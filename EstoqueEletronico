public class Eletronico extends Produto {
    public Eletronico(String nome, double preco) {
        super(nome, preco);
    }

    @Override
    public void ligar() {
        System.out.println("Eletronico " + getNome() + " ligado.");
    }

    @Override
    public void conectarWifi() {
        System.out.println("Eletronico " + getNome() + " conectado ao Wi-Fi.");
    }
}
